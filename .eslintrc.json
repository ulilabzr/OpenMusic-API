{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "no-unused-vars": "off"
  },
  "ignorePatterns": ["node_modules/"]
}
